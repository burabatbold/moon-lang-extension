{
  "name": "Moon Dark Theme",
  "type": "dark",
  "colors": {
    "editor.background": "#1E1E1E",
    "editor.foreground": "#D4D4D4"
  },
  "tokenColors": [
    {
      "name": "Keywords",
      "scope": "keyword.control.mn",
      "settings": {
        "foreground": "#569CD6"
      }
    },
    {
      "name": "Functions",
      "scope": "entity.name.function.mn",
      "settings": {
        "foreground": "#DCDCAA"
      }
    },
    {
      "name": "Types",
      "scope": "storage.type.mn",
      "settings": {
        "foreground": "#4EC9B0"
      }
    },
    {
      "name": "Strings",
      "scope": "string.quoted.double.mn",
      "settings": {
        "foreground": "#CE9178"
      }
    },
    {
      "name": "Numbers",
      "scope": "constant.numeric.mn",
      "settings": {
        "foreground": "#B5CEA8"
      }
    },
    {
      "name": "Operators",
      "scope": "keyword.operator.mn",
      "settings": {
        "foreground": "#D4D4D4"
      }
    },
    {
      "name": "Comments",
      "scope": "comment.line.double-slash.mn",
      "settings": {
        "foreground": "#6A9955"
      }
    }
  ]
}
